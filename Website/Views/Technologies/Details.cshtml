@model Website.Models.Technology

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}


<div>
    <dl class="row">
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <hr />
        <dd class = "col-sm-10">
            <img src="~/Image/Technologys/@Html.DisplayFor(model => model.image)" width="150px" height="150px" />
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="text-info" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index" class="text-info">Back</a>
</div>
