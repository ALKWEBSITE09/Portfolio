<body data-spy="scroll" data-target=".navbar" data-offset="51">
    <div class="wrapper">
        <div class="sidebar">
            <div class="sidebar-header">
                <img src="~/admin/img/photo.jpeg" alt="Image">
            </div>
            <div class="sidebar-content">
                <nav class="navbar navbar-expand-md bg-dark navbar-dark">
                    <a href="#" class="navbar-brand">ALK Admin</a>
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <ul class="nav navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link " asp-action="Dashboard" asp-controller="Admin">Dashboard<i class="fa fa-home"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Skills">Skills<i class="fa fa-address-card"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Technologies">Services<i class="fa fa-chalkboard"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Projects">Projects<i class="fa fa-tasks"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Courses">Courses<i class="fa fa-layer-group"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Experiences">Experienses<i class="fa fa-flask"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Resumes">Resume<i class="fa fa-star"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Contacts">Contact<i class="fa fa-envelope"></i></a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
            <div class="sidebar-footer">
                <a href="@TempData["yt"]"><i class="fab fa-youtube"></i></a>
                <a href="https://www.facebook.com/alpesh.khandekar.52?mibextid=ZbWKwL"><i class="fab fa-facebook-f"></i></a>
                <a href="https://www.instagram.com/kunal__09__04?igsh=MW55aGx6MGR3Y2V0Yw=="><i class="fab fa-linkedin-in"></i></a>
            </div>
            
        </div>
        
        <div class="content">
            <nav>
                <div class="row justify-content-end">
                    <a class="text-black" asp-action="Details" asp-controller="Profiles" asp-route-id="1">Account</a> 
                    <a style="margin-left:1em;margin-right:1em;">|</a> 
                    <a asp-action="Logout" asp-controller="Admin" class="text-black">Log Out</a>
                </div>
            </nav>
            <div class="container">
                @RenderBody()
            </div>
        </div>
    </div>

    <!-- Back to Top -->
    <a href="#" class="back-to-top"><i class="fa fa-angle-double-up"></i></a>

    <!-- JavaScript Libraries -->
