
<!DOCTYPE html>
<html lang="en">
<head>
    <title>ALKWEBSITE</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900" rel="stylesheet">

    <link rel="stylesheet" href="~/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="~/css/animate.css">

    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/css/magnific-popup.css">

    <link rel="stylesheet" href="~/css/aos.css">

    <link rel="stylesheet" href="~/css/ionicons.min.css">

    <link rel="stylesheet" href="~/css/flaticon.css">
    <link rel="stylesheet" href="~/css/icomoon.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/mycss.css"/>
</head>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">


    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar ftco-navbar-light site-navbar-target" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" asp-action="Index" asp-controller="Home">ALK</a>
            <button class="navbar-toggler js-fh5co-nav-toggle fh5co-nav-toggle" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span style="color:rgba(255, 255, 255, 0.5);margin-right:1em;" class="oi oi-menu"></span>
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav nav ml-auto">
                    <li class="nav-item"><a asp-action="Index" asp-controller="Home" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Index"?"active":"Home")"><span>Home</span></a></li>
                    <li class="nav-item"><a asp-action="About" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="About"?"active":"Index")"><span>About</span></a></li>
                    <li class="nav-item"><a asp-action="Resume" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Resume"?"active":"Index")"><span>Resume</span></a></li>
                    <li class="nav-item"><a asp-action="Skill" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Skill"?"active":"Index")"><span>Skills</span></a></li>
                    <li class="nav-item"><a asp-action="Service" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Service"?"active":"Index")"><span>Services</span></a></li>
                    <li class="nav-item"><a asp-action="Project" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Project"?"active":"Index") @(ViewContext.RouteData.Values["Action"].ToString()=="ProjectVideo"?"active":"Index")"><span>Projects</span></a></li>
                    <li class="nav-item"><a asp-action="Course" asp-controller="Index" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Course"?"active":"Index") @(ViewContext.RouteData.Values["Action"].ToString()=="CourseDetail"?"active":"Index")"><span>Cources</span></a></li>
                    <li class="nav-item"><a asp-action="Create" asp-controller="Contacts" class="nav-link @(ViewContext.RouteData.Values["Action"].ToString()=="Create"?"active":"Contacts")"><span>Contact</span></a></li>
                </ul>
            </div>
        </div>
    </nav>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    


    <footer class="ftco-footer ftco-section">
        <div class="container">
            <div class="row">
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">About</h2>
                        <p>@TempData["desc2"]</p>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                            <li class="ftco-animate" ><a target="_blank" href="@TempData["yt"]"><span class="icon-youtube-play"></span></a></li>
                            <li class="ftco-animate"><a target="_blank" href="https://www.facebook.com/alpesh.khandekar.52?mibextid=ZbWKwL"><span class="icon-facebook"></span></a></li>
                            <li class="ftco-animate"><a target="_blank" href="https://www.instagram.com/kunal__09__04?igsh=MW55aGx6MGR3Y2V0Yw=="><span class="icon-instagram"></span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4 ml-md-4">
                        <h2 class="ftco-heading-2">Links</h2>
                        <ul class="list-unstyled">
                            <li><a asp-action="Index" asp-controller="Home"><span class="icon-long-arrow-right mr-2"></span>Home</a></li>
                            <li><a asp-action="About" asp-controller="Index"><span class="icon-long-arrow-right mr-2"></span>About</a></li>
                            <li><a asp-action="Service" asp-controller="Index"><span class="icon-long-arrow-right mr-2"></span>Services</a></li>
                            <li><a asp-action="Project" asp-controller="Index"><span class="icon-long-arrow-right mr-2"></span>Projects</a></li>
                            <li><a asp-action="Index" asp-controller="Contacts"><span class="icon-long-arrow-right mr-2"></span>Contact</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Services</h2>
                        <ul class="list-unstyled">
                            <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Web Design</a></li>
                            <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Web Development</a></li>
                            <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Animation</a></li>
                            <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Data Science</a></li>
                            <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Graphic Design</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Have a Questions?</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li><span class="icon icon-map-marker"></span><span class="text">@TempData["add2"]</span></li>
                                <li><a href="#"><span class="icon icon-phone"></span><span class="text">@TempData["phone2"]</span></a></li>
                                <li><a href="#"><span class="icon icon-envelope"></span><span class="text">@TempData["email2"]</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
    </footer>



    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.easing.1.3.js"></script>
    <script src="~/js/jquery.waypoints.min.js"></script>
    <script src="~/js/jquery.stellar.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <script src="~/js/aos.js"></script>
    <script src="~/js/jquery.animateNumber.min.js"></script>
    <script src="~/js/scrollax.min.js"></script>

    <script src="~/js/main.js"></script>

</body>
</html>